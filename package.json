{
  "name": "adze",
  "version": "0.4.0",
  "description": "A library for shaping your logs.",
  "main": "dist/adze.js",
  "sideEffects": false,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "analyze": "tsc; webpack --env.production --env.withAnalysis",
    "prebuild": "rm -rf ./types && mkdir types",
    "build": "tsc && webpack --env.development",
    "prebuild:production": "rm -rf ./types && mkdir types",
    "build:production": "tsc; webpack --env.production",
    "demo:browser": "http-server -o -p 8082",
    "demo:node": "ts-node ./demo.ts",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "ava",
    "watch": "webpack --env.development --watch"
  },
  "homepage": "http://adzejs.com/",
  "repository": {
    "type": "git",
    "url": "git@github.com:AJStacy/Adze.git"
  },
  "keywords": [
    "log",
    "logging"
  ],
  "types": "types/src/index.d.ts",
  "author": "Andrew Stacy",
  "license": "Apache-2.0",
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@ava/typescript": "^1.1.1",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jsdom": "^16.2.3",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^12.7.5",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "ava": "^3.15.0",
    "babel-loader": "^8.2.2",
    "browser-env": "^3.3.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "http-server": "^0.11.1",
    "if-webpack-plugin": "^1.0.5",
    "jsdom": "^16.3.0",
    "prettier": "2.2.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "vuepress": "^1.5.3",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.2.3",
    "window": "^4.2.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "browser-process-hrtime": "^1.0.0",
    "chalk": "^4.1.0",
    "lodash": "^4.17.20"
  }
}
